<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	
	
	<bean class="com.capg.beans.SBU" id="sbu">
	<property name="sbuId" value="1001"></property>
	<property name="sbuName" value="kiran"></property>
	<property name="sbuHead" value="Product Engineering Services"></property>
	<property name="empList">
	<list>
	<ref bean="emp1"/>
	<ref bean="emp2"/>
	</list></property>
	</bean>
	<bean class="com.capg.beans.Employee" id="emp1">
	<property name="empId" value="1001"/>
	<property name="empName" value="Alok"/>
	<property name="salary" value="50000"/>
	<property name="businUnit" value="B-SE"/>
	<property name="age" value="25"/>
	</bean>
	<bean class="com.capg.beans.Employee" id="emp2">
	<property name="empId" value="1002"/>
	<property name="empName" value="Alok"/>
	<property name="salary" value="50000"/>
	<property name="businUnit" value="B-SE"/>
	<property name="age" value="25"/>
	</bean>
	
</beans>
